{
  "builds": [
    {
      "src": "django_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/blog/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "django_project/wsgi.py"
    }
  ]
}